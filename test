 private parseFinancialYear(fyString) {
    // Extract the starting year from the string (e.g., "22" from "FY 22-23")
    let year = parseInt(fyString.split(" ")[1].split("-")[0], 10); 
    // Convert it to a full year (e.g., "2022" from "22")
    return 2000 + year;
  }

private parseFinancialYear(fyString) {
    let yearPart = fyString.split(" ")[1];
    
    if (yearPart.includes("-")) {
        // Format: FY 24-25
        let year = parseInt(yearPart.split("-")[0], 10);
        return 2000 + year;
    } else {
        // Format: FY 2025
        return parseInt(yearPart, 10);
    }
}
